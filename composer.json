{
    "version": "1.0.0",
    "name": "sunwarul/rp",
    "description": "Role and Permissions Management Package for Laravel Including UI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunwarul",
            "email": "sunwarul.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "spatie/laravel-permission": "^6.16"
    },
    "autoload": {
        "psr-4": {
            "Sunwarul\\RP\\": "src/"
        }
    },
    "extra": {
